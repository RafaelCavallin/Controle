     produtos.`Estado` AS produtos_Estado,
     produtos.`DataCadastro` AS produtos_DataCadastro,
     produtos.`Imagem` AS produtos_Imagem
FROM
     `produtos` produtos
WHERE
    produtos.`EstoqueMinimo` >  produtos.`Quantidade`]]>
